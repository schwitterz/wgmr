	journal = {Optics Express},
	author = {Ng, Vincent and Azeem, Farhan and Trainor, Luke S. and Schwefel, Harald G. L. and Coen, Stéphane and Erkintalo, Miro and Murdoch, Stuart G.},
	month = feb,
